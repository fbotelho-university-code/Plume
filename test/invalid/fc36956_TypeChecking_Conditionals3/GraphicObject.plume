abstract class GraphicObject extends Object is
    field x : Integer
    field y : Integer

    method moveTo : (newX : Integer, newY : Integer) -> Boolean is
        true
    
    abstract method draw : () -> Boolean
    
    abstract method resize : () -> Boolean

		