class Train extends Object is
	field maxPPL : Integer
	field id : Integer
	field owner : Company
	
	method getOwner : () -> Company is  
		owner
	  
	method changeOwner : ( newOwner : Company ) -> Train is  
		new Train(maxPPL,id,newOwner)		    	
	
	method getId : () -> Integer is
		id
	method getMax : () -> Integer is 
		maxPPL