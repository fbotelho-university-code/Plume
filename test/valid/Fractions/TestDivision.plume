/**
 * Tests division with numbers
 *
 * @author Robin Burgess 36815
 */
class TestDivision extends Object is

	method main : () -> String is
		(new IntegerNumber(5)).divideBy(new RationalNumber(3,2).divideBy(new RationalNumber(4,2)))
				.divideBy(new IntegerNumber(2)).toString()
		
	method override toString : () -> String is
		"Testing Division"