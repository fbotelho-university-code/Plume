 class LinkedList extends Object is

	field firstNode : Node
	
	method getFirstNode : () -> Node is
		firstNode
		
	method getListSize : () -> Integer is
		if !firstNode.hasNextNode()
		then 0
		else firstNode.countMeOn()	
	