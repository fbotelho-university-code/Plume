class TestPriorityQueueRemoveInsert extends Object is

  method main : () -> String is
  	new EmptyPriorityQueue().insert(1, "Hello").insert(5, "World")
  		.remove().insert(2, "We").insert(2, "Thank").insert(4, "You").toString()
   	// expected output: "(2:We) | (2:Thank) | (4:You) | (5:World) | _"