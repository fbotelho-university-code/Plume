class Castiel extends Object is
	field power : Integer
	method triplecast : () -> Integer is  
		(((power as Integer) as Object) as Integer)
	
	method kidingCasts : () -> Integer is  
		(( (4 as Integer ) + (3 as Integer) - (triplecast() as Integer) ) as Integer)
