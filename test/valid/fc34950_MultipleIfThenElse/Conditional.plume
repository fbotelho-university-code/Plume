class Conditional extends Object is
	  
	field ret : Integer
	
	method run : () -> Integer is
	 	if 0 == 0 + 1 then 
	 		ret
	 	else
	 		if true then 
	 			if !false then	
	 				3 
	 			else
	 				ret
	 		else
	 			if !(!(!(!(!false)))) then
	 				ret + 1
	 			else
					0
	